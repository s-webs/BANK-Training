{
	"env": {
		"browser": true,
		"node": true,
		"es2021": true
	},
	"extends": "eslint:recommended",
	"overrides": [],
	"parser.options": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"settings": {
		"import/resolver": {
			"alias": {
				"map": [["@", "./src"]]
			},
			"extensions": [".js"]
		}
	}
}
